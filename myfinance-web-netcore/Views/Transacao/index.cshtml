@{
    ViewData["Title"] = "Transações financeiras";
}

<h1>@ViewData["Title"]</h1>

<div>
    <a href="Transacao/Cadastro" class="btn btn-primary btn-lg">Criar item de Transações financeira</a>

    <table class="table">
        <thead>
            <tr>
            <th scope="col">Id</th>
            <th scope="col">Descrição</th>
            <th scope="col">Tipo</th>
            <th scope="col">#</th>
            <th scope="col">#</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th scope="row">1</th>
                <td>Mark</td>
                <td>Otto</td>
                <td><button type="button" class="btn btn-outline-warning">Editar</button></td>
                <td><button type="button" class="btn btn-outline-danger">Deletar</button></td>
            </tr>
        </tbody>
    </table>
</div>